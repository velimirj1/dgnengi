{
  "name": "nengi-2d-demo",
  "version": "1.0.0",
  "description": "nengi 2d example game with client-side prediction and lag compensation",
  "type": "module",
  "scripts": {
    "start": "npx npm-run-all --parallel server client",
    "server": "npx nodemon ./server/index.js --ignore public",
    "client": "set NODE_OPTIONS=--openssl-legacy-provider && npx webpack-dev-server --config webpack.dev.js --host 0.0.0.0",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && npx webpack --config webpack.prod.js"
  },
  "author": "timetocode // Alex",
  "license": "Apache 2.0",
  "dependencies": {
    "nengi": "file:../nengi",
    "pixi.js": "5.1.5",
    "sat": "0.8.0"
  },
  "devDependencies": {
    "nodemon": "1.19.3",
    "npm-run-all": "4.1.5",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "4.41.1",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2",
    "webpack-merge": "4.2.2"
  }
}
